@model LUANVANTOTNGHIEP_VODUCANKHANG.Models.Phanquyen

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_AdminForAdminLayout.cshtml";
}

<h1 class="container-fluid pt-4 px-4">Create</h1>

<div class="container-fluid pt-4 px-4"  style="color:whitesmoke">
    <div class="row g-4">
        <div class="col-sm-12 col-xl-12"  style="color:whitesmoke">
            <div class="bg-secondary rounded h-100 p-4"  style="color:whitesmoke">
                <h6 class="mb-4">Tạo Quyền Truy Cập Mới</h6>
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Tenquyentruycap" class="control-label">Tên Quyền Truy Cập</label>
                        <input asp-for="Tenquyentruycap" class="form-control bg-white" />
                        <span asp-validation-for="Tenquyentruycap" class="text-danger"></span>
                    </div>
                    <br />
                    <div class="form-group">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<br />
<div class="container-fluid pt-4 px-4">
    <a asp-action="Index" class="btn btn-primary">Back to List</a>
</div>

